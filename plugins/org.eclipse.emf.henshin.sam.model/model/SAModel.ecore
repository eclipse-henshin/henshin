    <eClassifiers xsi:type="ecore:EClass" name="Match" eSuperTypes="#//samannotation/AnnotatedElem">
      <eOperations name="clear">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="body" value="this.getEdgeMatching().clear();&#xA;this.getNodeMatching().clear();"/>
        </eAnnotations>
      </eOperations>
      <eOperations name="copy" eType="#//samtrace/Match">
        <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
          <details key="body" value="Match result = SamtraceFactory.eINSTANCE.createMatch();&#xA;result.getNodeMatching().putAll(this.getNodeMatching());&#xA;result.getEdgeMatching().putAll(this.getEdgeMatching());&#xA;return result;"/>
        </eAnnotations>
      </eOperations>
