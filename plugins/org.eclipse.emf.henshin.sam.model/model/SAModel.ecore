  <eSubpackages name="samgraphcondition" nsURI="http://www.eclipse.org/emf/2015/Henshin/sam/samgraphcondition"
      nsPrefix="samgraphcondition">
    <eClassifiers xsi:type="ecore:EClass" name="ProxyNode" eSuperTypes="#//samgraph/Node">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="A &lt;code>ProxyNode&lt;/code> refers to a node that is implicitly part of the &lt;code>context&lt;/code> of a &lt;code>Quantification&lt;/code>. In terms of inclusion morphisms &lt;code>ProxyNode&lt;/code>s refer to quantified nodes of the domain. Conversely, quantified nodes belonging to the codomain, but not belonging to the domain of the inclusion morphism are added explicitly to the &lt;code>context&lt;/>."/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EReference" name="referencedNode" lowerBound="1"
          eType="#//samgraph/Node"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="GraphCondition" abstract="true" interface="true"
        eSuperTypes="#//samtypegraph/TypeGraphCondition #//samannotation/AnnotatedElem">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="A &lt;code>GraphCondition&lt;/code> is the abstract super type for graphical conditions (optionally mixed with attribute conditions) that can be attached to a &lt;code>RuleGraph&lt;/code> as left or right application condition or a &lt;code>TypeGraph&lt;/code> as constraint for all graphs typed over the type graph.&#xD;&#xA;A &lt;code>GraphCondition&lt;/code> denotes a constraint for the type graph if it is referenced  by the &lt;code>TypeGraph&lt;/code> via &lt;code>constraints&lt;/code> as a &lt;code>TypeGraphConstraint&lt;/code>. &#xD;&#xA;"/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="LogicalGCCoupling" eSuperTypes="#//samgraphcondition/GraphCondition">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="A &lt;code>GraphCondition&lt;code> may be a disjunction or conjunction (&lt;code>LogicalOperator&lt;/code>) of &lt;code>GraphCondition&lt;/code>s."/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="operator" eType="#//samgraphcondition/LogicalOperator"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="operands" lowerBound="2"
          upperBound="-1" eType="#//samgraphcondition/GraphCondition" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Quantification" eSuperTypes="#//samgraphcondition/GraphCondition">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="A &lt;code>Quantification&lt;/code> is the probably most important &lt;code>GraphCondition&lt;/code> to be used. &lt;br/>&#xD;&#xA;Either it looks for nodes and edges in the &lt;code>context&lt;/code> to exist or it looks for all occurrences of the nodes and edges in the &lt;code>context&lt;/code>.&#xD;&#xA;For this one occurrence or for all occurences, respectively, the &lt;code>nesting&lt;/code> &lt;code>GraphCondition&lt;/code> needs to hold.  &#xD;&#xA;An &lt;code>attributeCondition&lt;/code> may constrain the attribute values of the &lt;code>context&lt;/code> (TO BE CHECKED)."/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="quantor" eType="#//samgraphcondition/Quantor"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="nesting" lowerBound="1"
          eType="#//samgraphcondition/GraphCondition" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="context" lowerBound="1"
          eType="#//samgraph/Graph" containment="true"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="attributeCondition" eType="#//samalgebra/algebraUse/OperationUse"
          containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EEnum" name="Quantor">
      <eLiterals name="EXISTS"/>
      <eLiterals name="FORALL" value="1"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EEnum" name="LogicalOperator">
      <eLiterals name="DISJUNCTION"/>
      <eLiterals name="CONJUNCTION" value="1"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="TerminationCondition" eSuperTypes="#//samgraphcondition/GraphCondition">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="The nesting of a &lt;code>GraphCondition&lt;/code> can be terminated by instantiating this &lt;code>TerminationCondition&lt;/code> being true or false."/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="isTrue" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EBoolean"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="NegatedCondition" eSuperTypes="#//samgraphcondition/GraphCondition">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="A &lt;code>NegatedCondition&lt;/code> is false whenever the included &lt;code>GraphCondition&lt;/code> is false and the other way round."/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EReference" name="operand" eType="#//samgraphcondition/GraphCondition"
          containment="true"/>
    </eClassifiers>
  </eSubpackages>
  <eSubpackages name="samtrace" nsURI="http://www.eclipse.org/emf/2015/Henshin/sam/samtrace"
